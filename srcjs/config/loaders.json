[
  {
    "test": "\\.(js|jsx)$",
    "use": [
      "babel-loader"
    ],
    "exclude": "/node_modules/"
  },
  {
    "test": "\\.f7.(html|js|jsx)$",
    "use": [
      "babel-loader",
      "framework7-loader"
    ]
  },
  {
    "test": "\\.html$/i",
    "use": [
      "html-loader"
    ]
  },
  {
    "test": "\\.css$",
    "use": [
      "style-loader",
      "css-loader"
    ]
  }
]
